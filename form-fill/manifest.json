{
    "manifest_version": 2,
    "name": "活动页面－表单自动填充",
    "version": "1.0",
    "description": "让测试变的更简单",
    "content_scripts": [
        {
            "matches": ["*://edmp.test.com/index/*","*://edmp.localhost/index/*"],
            "js": ["js/fill.js"]
        }
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "表单填充"
    }
}